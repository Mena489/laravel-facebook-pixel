{
    "name": "mina/laravel-facebook-pixel",
    "description": "Meta pixel integration for Laravel",
    "keywords": [
        "combindma",
        "meta-pixel",
        "laravel"
    ],
    "homepage": "https://github.com/combindma/laravel-facebook-pixel",
    "license": "MIT",
    "authors": [
        {
            "name": "Combind",
            "email": "webmaster@combind.ma",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4",
        "facebook/php-business-sdk": "^16.0",
        "illuminate/contracts": "^8.0",
        "spatie/laravel-package-tools": "^1.11"
    },
    "require-dev": {
    },
    "autoload": {
        "psr-4": {
            "Combindma\\FacebookPixel\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Combindma\\FacebookPixel\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Combindma\\FacebookPixel\\FacebookPixelServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
